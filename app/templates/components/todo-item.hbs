{{#if editing}}
  {{input class="edit" value=todo.title action="submitTodo"}}
{{else}} 
{{input type="checkbox" class="toggle" checked=todo.complete}}
<label class="{{if todo.complete 'completed'}}" {{action "editTodo"
on="doubleClick"}}>{{todo.title}}</label>
<button class="destroy" {{action "deleteTodo"}}></button>
{{/if}}
